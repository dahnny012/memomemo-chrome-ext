{
  "name": "MemoMemo Companion",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add cards to memo memo on the fly",
  "homepage_url": "http://memomemo.xyz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
   "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "https://*/*"
  ]
}